{
  "title": "Stdlib: KEYS, VALUES, ENTRIES",
  "description": "Explore KEYS, VALUES, and ENTRIES on objects and lists.",
  "program": [
    "LET objectKeys = KEYS(input.user);",
    "LET objectValues = VALUES(input.user);",
    "LET objectEntries = ENTRIES(input.user);",
    "",
    "LET listKeys = KEYS(input.tags);",
    "",
    "OUTPUT {",
    "    object_keys: objectKeys,",
    "    object_values: objectValues,",
    "    object_entries: objectEntries,",
    "    list_keys: listKeys",
    "}"
  ],
  "input": {
    "user": {
      "id": 101,
      "role": "admin",
      "active": true
    },
    "tags": [
      "new",
      "beta",
      "owner"
    ]
  }
}