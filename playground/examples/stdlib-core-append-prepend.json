{
  "title": "Stdlib: APPEND, PREPEND, COLLECT, IS_OBJECT",
  "description": "Build new lists with APPEND/PREPEND and normalize iterables with COLLECT.",
  "program": [
    "LET appended = APPEND(msg.scores, 99);",
    "LET prepended = PREPEND(msg.scores, -1);",
    "LET walked = WALK(msg.profile);",
    "LET collected = COLLECT(walked);",
    "LET profileIsObject = IS_OBJECT(msg.profile);",
    "",
    "OUTPUT {",
    "    appended: appended,",
    "    prepended: prepended,",
    "    collected_walk: collected,",
    "    profile_is_object: profileIsObject",
    "}"
  ],
  "input": {
    "scores": [
      7,
      8,
      9
    ],
    "profile": {
      "name": "Nia",
      "teams": [
        "growth",
        "research"
      ]
    }
  }
}
